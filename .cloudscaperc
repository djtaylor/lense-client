# Cloudscape Environment
# 
# Shell script that sets up environment variables. If you want to change
# the installation location of Cloudscape, you should adjust the CLOUDSCAPE_BASE
# value in this file.

# Executables
export PATH=$PATH:~/bin

# Cloudscape Base Directory
export CLOUDSCAPE_BASE=~

# Cloudscape Python Path
export PYTHONPATH="$CLOUDSCAPE_BASE/python"

# API Client Connection Settings
# API user
#export CS_API_USER="cloudscape"
# API group
#export CS_API_GROUP="cloudscape"
# API key
#export CS_API_KEY="<your_key_here>"